{
  "StartAt": "billing",
  "States": {

        "billingError": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:eu-central-1:127306523332:function:billingError",
            "End": true
        },
            "billing": {
            "Type": "Task",
            "Resource": "arn:aws:lambda:eu-central-1:127306523332:function:billing",
            "Catch": [{
                "ErrorEquals": ["States.ALL"],
                "Next": "billingError"
            }],
            "Next": "billingMailer"
        },
    
    "billingMailer": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:eu-central-1:127306523332:function:billingMailer",
                "Catch": [{
                "ErrorEquals": ["States.ALL"],
                "Next": "billingError"
            }],
            "Next": "billingCompletion"
    },
        "billingCompletion": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:eu-central-1:127306523332:function:billingCompletion",
                "Catch": [{
                "ErrorEquals": ["States.ALL"],
                "Next": "billingError"
            }],
            "End": true
    }
  }
}
